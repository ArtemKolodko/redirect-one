{
  "name": "Simple .1 Redirect",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
